{
  "name": "WCL Threat Link",
  "description": "Link Warcraft logs to TBCC threat graph",
  "version": "1.3",
  "manifest_version": 2,
  "content_scripts" : [
    {
      "matches": ["*://*.classic.warcraftlogs.com/reports/*"],
      "js": ["addButton.js"]
    },
    {
      "matches": ["*://*.vanilla.warcraftlogs.com/reports/*"],
      "js": ["addButtonEra.js"]
    }
  ]
}